{
  "ReRoutes": [
    {
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "feedback",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/f/{everything}",
      "UpstreamHttpMethod": [
        "GET",
        "POST",
        "OPTIONS",
        "DELETE",
        "PUT",
        "PATCH"
      ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer",
        "AllowedScopes": []
      },
      "AddHeadersToRequest": {
        "USER_ID": "Claims[http://schemas.xmlsoap.org/ws/2005/05/identity/claims/nameidentifier] > value"
      }
    },
    {
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "people",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/p/{everything}",
      "UpstreamHttpMethod": [
        "GET",
        "POST",
        "OPTIONS",
        "DELETE",
        "PUT",
        "PATCH"
      ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer",
        "AllowedScopes": []
      },
      "AddHeadersToRequest": {
        "USER_ID": "Claims[http://schemas.xmlsoap.org/ws/2005/05/identity/claims/nameidentifier] > value"
      }
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5005"
  }
}